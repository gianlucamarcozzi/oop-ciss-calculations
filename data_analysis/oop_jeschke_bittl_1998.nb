    RowBox[{"k22", " ", "=", " ", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "dmJ", " ", "\[Tau]"}], "]"}]}], " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "q", " ", "\[Tau]"}], "]"}], " ", 
      RowBox[{"Sin", "[", "\[Xi]", "]"}]}]}], ";"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Szy", " ", "+", " ", "Syz"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"k32", " ", "=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Cos", "[", "\[Xi]", "]"}], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "dmJ", " ", "\[Tau]"}], "]"}]}], "+", " ", 
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"2", " ", "dmJ", " ", "\[Tau]"}], "]"}], " ", 
       RowBox[{"Sin", "[", "\[Xi]", "]"}], 
       RowBox[{"Sin", "[", 
        RowBox[{"2", " ", "q", " ", "\[Tau]"}], "]"}]}]}]}], ";"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Sxz", " ", "-", " ", "Szx"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"k42", " ", "=", " ", 
     RowBox[{
      RowBox[{"Sin", "[", 
       RowBox[{"2", " ", "dmJ", " ", "\[Tau]"}], "]"}], 
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "q", " ", "\[Tau]"}], "]"}], " ", 
      RowBox[{"Sin", "[", "\[Xi]", "]"}]}]}], " ", ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Fast", " ", "decaying", " ", "terms", " ", "including", " ", "q", " ", 
     "are", " ", 
     RowBox[{"neglected", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"k12", " ", "=", " ", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"Cos", "[", "\[Xi]", "]"}]}], " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"2", " ", "dmJ", " ", "\[Tau]"}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"k22", " ", "=", " ", "0"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"k32", " ", "=", " ", 
     RowBox[{
      RowBox[{"Cos", "[", "\[Xi]", "]"}], " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "dmJ", " ", "\[Tau]"}], "]"}]}]}], ";"}], 
