       RowBox[{"sya", " ", "-", " ", "syb"}], ",", " ", 
       RowBox[{"syz", " ", "+", " ", "szy"}], ",", " ", 
       RowBox[{"sxa", " ", "+", " ", "sxb"}], ",", " ", 
       RowBox[{"sxz", " ", "-", " ", "szx"}], ",", " ", 
       RowBox[{"sya", " ", "+", " ", "syb"}], ",", " ", 
       RowBox[{"syz", " ", "-", " ", "szy"}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Transform", " ", "from", " ", "product", " ", "basis", " ", "to", " ", 
     "eigenbasis"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"propagatorU\[Xi]", "[", "a_", "]"}], " ", ":=", " ", 
     RowBox[{"propagator", "[", 
      RowBox[{"a", ",", " ", 
       RowBox[{"sxy", " ", "-", " ", "syx"}], ",", " ", 
       RowBox[{"\[Xi]", "/", "2"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"pp1", " ", "=", " ", 
