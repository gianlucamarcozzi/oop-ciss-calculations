  RowBox[{"(*", " ", 
   RowBox[{"Single", " ", "quantum", " ", "operators"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"pp", " ", "=", " ", 
     RowBox[{"{", 
